{
  "name": "wreathe-js/laravel",
  "type": "library",
  "description": "The Laravel adapter for wreathe.",
  "keywords": [
    "laravel",
    "preact",
    "react",
    "vite",
    "vue",
    "wreathe",
    "wreathe-js"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Boes",
      "homepage": "https://robertbo.es"
    },
    {
      "name": "JÃ¶rn Spangenberg",
      "homepage": "https://jrson.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wreathe\\": "src"
    },
    "files": [
      "./helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Wreathe\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2|~8.0.0|~8.1.0|~8.2.0",
    "ext-json": "*",
    "laravel/framework": "^6.0|^7.0|^8.74|^9.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "orchestra/testbench": "^4.0|^5.0|^6.4|^7.0",
    "mockery/mockery": "^1.3.3",
    "phpunit/phpunit": "^8.0|^9.5.8"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wreathe\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
